{
  "name": "challenge-quero-educacao",
  "version": "1.0.0",
  "description": "challenge quero educação",
  "main": "src/server.js",
  "repository": "git@github.com:andregaldino/challenge-quero-educacao.git",
  "author": "André Galdino <andre.galdinolima@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node src/index.js",
    "lint": "npx eslint . --fix",
    "db:create": "npx sequelize-cli db:create",
    "db:seed": "npx sequelize-cli db:seed:all",
    "pretest:acceptance": "cross-env NODE_ENV=test yarn db:create && cross-env NODE_ENV=test npx sequelize-cli db:migrate &&  cross-env NODE_ENV=test yarn db:seed",
    "posttest:acceptance": "cross-env NODE_ENV=test npx sequelize db:drop",
    "test:acceptance": "cross-env NODE_ENV=test mocha __tests__/acceptance/api.js",
    "test": "cross-env NODE_ENV=test mocha ./__tests__/**/*.spec.js && yarn test:acceptance"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "mocha": "^8.1.3",
    "prettier": "^2.1.1",
    "sequelize-cli": "^6.2.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mysql2": "^2.1.0",
    "sequelize": "^6.3.5"
  }
}
